# IBM WatsonX Configuration (required for AI Race Engineer)
# Get credentials from: https://cloud.ibm.com/catalog/services/watsonx-ai

WATSONX_API_KEY=your_watsonx_api_key_here
WATSONX_PROJECT_ID=your_project_id_here
WATSONX_URL=https://us-south.ml.cloud.ibm.com

# Optional: IBM Watson Speech Services (for future voice integration)
# WATSON_TTS_API_KEY=your_tts_api_key_here
# WATSON_TTS_URL=https://api.us-south.text-to-speech.watson.cloud.ibm.com
# WATSON_STT_API_KEY=your_stt_api_key_here
# WATSON_STT_URL=https://api.us-south.speech-to-text.watson.cloud.ibm.com

# Optional: LiveKit (for future WebRTC integration)
# LIVEKIT_URL=wss://your-livekit-instance.livekit.cloud
# LIVEKIT_API_KEY=your_livekit_api_key_here
# LIVEKIT_API_SECRET=your_livekit_api_secret_here

# Optional: LangSmith (for LLM observability)
# LANGSMITH_API_KEY=your_langsmith_api_key_here

# Application Settings
LLM_PROVIDER=watsonx
LOG_LEVEL=INFO
DEBUG=False
